<script language="javascript" type="text/javascript">
    $(".loadMoreButton").click(function () {
        var self = $(this);
        var id = self.attr('data');
        var maxCitations = $("#UsageTable" + id + " tr").length;

        $("#UsageTable" + id + " tr:hidden").slice(0, Math.min(10, $("#UsageTable" + id + " tr:hidden").length)).css('display', 'table-row');
        var numVisible = $("#UsageTable" + id + " tr:visible").length;
        $("#displayCount" + id).text(numVisible);
        if (numVisible == maxCitations)
            self.css('display', 'none')
        SetNavBarContentHeight();

    });
    $(document).ready(function () {
        addDownloadLinksToFigures();
        expandArticleImagesInModal();
    });

    //Add popout modal feature to all images within the figures & tables tab.
    //This creates a modal of the current figure, the title, and the description, along with a download link.
    function expandFigures(figureBoxId)
    {

        var figureModal = $("#" + figureBoxId).clone().css("padding", "0 30px 30px 30px");

        figureModal.addClass("activeDialog figureDialog").dialog(
            {
                width: $(window).width() * 0.5,
                maxHeight: $(window).height() * 0.8,
                create: function () {
                    //Move the dialog's largeImage parent to after the captiomn, and add responsive styles.
                    $(this).find("img").eq(1).css({ height: "auto", maxWidth: "80%", maxHeight: "80%" }).parent().insertAfter($(this).find("text").last().parent().addClass("figureDialogCaption"));
                    $(this).css("width", $(this).find("img").eq(1).css("width"));
                },
                modal: true,
                resizable: false,
                draggable: false,
                closeOnEscape: true,
                close: function () { $(this).remove(); },
                title: figureModal.find(".ArticleContentBoldText").first().text()
            }
        ).css("margin-top", "").find(".ArticleContentBoldText").first().remove();

        $(".activeDialog .toggleLinks").toggle();

    }

</script></div>
