<script language="javascript" type="text/javascript">

    function ShowFolderCreateDialog(doi, type) {

        
        $("#CreateFolderPaperId").val(doi);
        $("#CreateFolderPaperType").val(type);
        $("#CreateFolderErrorText").html("");

        $('.ui-dialog-content').dialog('close');
        $("#divPopupCreateFolder").dialog({
            width: 320,
            height: 400,
            modal: true,
            resizable: false,
            draggable: false,
            closeOnEscape:true,
            zIndex: 2010,
            title: ' CREATE FOLDER'
        });

        return false;
    }
    			
    $("form#postform").submit(function (e)
    {
        e.preventDefault();
        $("#ProcessedDescription").val(encodeURIComponent($("#folderDescription").val()));
        $("#CreateFolderErrorText").html("");

        var folderName = $("#folderName").val();

        if ((!folderName.replace(/\s/g, '').length)) {
            $("#CreateFolderErrorText").html("Folder Name: Name must contain text");
        }

        else if (ContainsBadCharacters(folderName)) {
            $("#CreateFolderErrorText").html("Folder Name: Please use only letters, numbers, spaces, hyphens, underscores.");
        }
        else {

            $.ajax({
                type: "post",
                url: $(this).attr("action"),
                data: $(this).serialize(),
                contenttype: 'application/x-www-form-urlencoded; charset=utf-8',
                datatype: 'json',
                statusCode: {
                    500: function () {
                        $("#CreateFolderErrorText").html("Please enter a folder name without HTML tags");
                    }
                }
            }).done(function (result) {
                if (result) {
                    location.reload();
                }
            });

        }
    });

</script>



                </div>

                <div class="PAHArticleCol" id="mainArticle" style="">
                    <div id="divArticleContent">
        <script language="javascript" type="text/javascript">
            function ArticleContentVisibleToggle(obj, divID) {
            var div = document.getElementById(divID);
            if (div.style.display == 'none') {
                div.style.display = 'block';
                obj.innerText = 'Load Less -';
            }
            else {
                div.style.display = 'none';
                obj.innerText = 'Load More +';
            }
            return false;
        }
        function ExcerptVisibleToggle(className, divID) {

            var div = document.getElementById(divID);
            var obj = $("." + className);

            if (div.style.display == 'none') {
                div.style.display = 'block';
                    //obj.text('Hide Excerpt -');
                    $('#aToggleText').text('Hide Excerpt');
                    $('#aToggleSign').text('-');
            }
            else {
                div.style.display = 'none';
                    //obj.text('View Excerpt +');
                    $('#aToggleText').text('View Excerpt');
                    $('#aToggleSign').text('+');
            }
            return false;
        }
