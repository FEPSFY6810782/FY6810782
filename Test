<style>
    .screenReaderText {
        border: 0;
        clip: rect(0 0 0 0);
        height: 1px;
        margin: -1px;
        overflow: hidden;
        padding: 0;
        position: absolute;
        width: 1px;
    }
</style>
<script language="javascript" type="text/javascript">

    $(document).ready(function () {       

        $("#downloadCitation").click(function (elem) {
                    console.log("clicked download citation");

                    var contentTypeInput = $('input[name=contentType]:checked');
                    var selectedContentType = contentTypeInput.attr("data-selected");

                    var formatTypeInput = $('input[name=formatType]:checked');
                    var selectedFormatType = formatTypeInput.attr("data-selected");

                    var obj = {
                                contentType: selectedContentType,
                                formatType: selectedFormatType,
                                referenceType: 'conference',
                                urlId: '10.1117/12.2679414'
                            };


            $.ajax({
                    type: "POST",
                    url: "/citation/download",
                    data: JSON.stringify(obj),
                    contentType: "application/json; charset=utf-8",                                
                    traditional: true,

                    success: function (response) {
                            //console.log("success, filename: " + response);                        
                            //window.location.assign("citation/download/" + response);

                            var downloadUrl = "citation/download/" + response;
                            var downloadFrame = document.createElement("iframe");
                            downloadFrame.setAttribute('src', downloadUrl);
                            downloadFrame.setAttribute('class', "screenReaderText");
                            document.body.appendChild(downloadFrame); 

                        },
                    failure: function (response) {
                            console.log("fail, filename: " + response);
                        },
                    error: function (response) {
                            console.log("error, filename: " + response);
                        }
                    });

        });
    });
    function ShowDownloadCitationDialog() {
        $('.divPopup').hide();
       //var  x = $(this).position().left - (document.scrollLeft);
       //var  y = $(this).position().top - (document.scrollTop);
        $("#divPopupDownloadCitation").dialog(
                        {
                            width: 420,
                            //height: 500,
                            modal: true,
                            resizable: false,
                            draggable: false,
                            closeOnEscape:true,
                            zIndex: 999,
                            title: 'DOWNLOAD CITATION:',

                        }
        );
        return false;
    }

</script>


        </div>
    </div>
</div>

<script>
    //highlight the current tab
        
            var currentTabElement = $('li:has(a[data-action="ArticleLink"])');
            var removeActiveClass = "#divMENUBAR " + "li";

            if (currentTabElement.length > 0)
            {
                $(removeActiveClass).removeClass('active');
                $(currentTabElement).addClass('active');
            }
        
</script>


