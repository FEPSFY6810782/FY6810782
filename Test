    <h2>Search In:</h2>
    <div id="SearchInAdvSearchContainer" class="advSearchContainer">
        <div id="advSearchContainerProceedings_Article" style="margin-bottom:12px">
            <div style="width:160px;display:inline-block">
                <input type="checkbox" id="advSearchProceedings_Article" class="advSearchPubType" value="Proceedings_Article">
                <label for="advSearchProceedings_Article"><span class="displayCheckbox" tabindex="0"></span>Proceedings</label>
            </div>
            <div class="advSearchNumberFilterContainer">
                <label for="advSearchProceedingsVolume">Volume</label>
                <input id="advSearchProceedingsVolume" class="pubFacetTextbox" type="text" placeholder="All" maxlength="5" onchange="$('#advSearchProceedings_Article').prop('checked', true).change()">
            </div>
            
            
        </div>
        
        <div id="advSearchContainerJournal_Article" style="margin-bottom:12px">
            <div>
                <div style="width:160px;display:inline-block">
                    <input type="checkbox" id="advSearchJournal_Article" class="advSearchPubType" value="Journal_Article">
                    <label for="advSearchJournal_Article"><span class="displayCheckbox" tabindex="0"></span>Journals +</label>
                </div>
                <div class="advSearchNumberFilterContainer">
                    <label for="advSearchJournalVolume">Volume</label>
                    <input id="advSearchJournalVolume" class="pubFacetTextbox" type="text" placeholder="All" maxlength="5" onchange="$('#advSearchJournal_Article').prop('checked', true).change()">
                </div>
                <div class="advSearchNumberFilterContainer">
                    <label for="advSearchJournalIssue">Issue</label>
                    <input id="advSearchJournalIssue" class="pubFacetTextbox" type="text" placeholder="All" maxlength="3" onchange="$('#advSearchJournal_Article').prop('checked', true).change()">
                </div>
