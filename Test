<script>
    $(document).ready(function ()
    {
        var $form = $("#SignInForm-TopNav");

        $form.find(".checkboxShowHidePwd").on("change", function (e)
        {
            e.preventDefault();

            var $pwd = $form.find(".pwdSignIn");
            $pwd.attr("type", $pwd.attr("type") === "password" ? "text" : "password");
        });

        $form.on("submit", function (e)
        {
            e.preventDefault();

            var $divSigninDialogWait = $form.find(".divSigninDialogWait");
            var $btnSigninDialogSubmit = $form.find(".btnSigninDialogSubmit");
            var $SignInErrorMessage = $form.find(".SignInErrorMessage");
            $divSigninDialogWait.show();
            $btnSigninDialogSubmit.hide();
            $SignInErrorMessage.text("").hide();

            $.ajax({
                type: "POST",
                url: '/Account/SignIn',
                data: $(this).serialize(),
                dataType: 'json',
                success: function (result)
                {
                    if (result && result.Success)
                    {
                        if (result.returnURL)
                        {
                            var a = document.createElement("a");
                            a.href = result.returnURL;
                            a.search += (a.search ? "&" : "?") + "returnUrl=" + encodeURIComponent(location.href);
                            location = a.href;
                        }
                        else
                        {
                            location.reload();
                        }
                    }
                    else
                    {
                        $divSigninDialogWait.hide();
                        $btnSigninDialogSubmit.show();
                        var errorMessage = result && result.Message || "Sign in failure. Please try again.";
                        $SignInErrorMessage.text(errorMessage).fadeIn();
                    }
                },
                error: function ()
                {
                    $divSigninDialogWait.hide();
                    $btnSigninDialogSubmit.show();
                    $SignInErrorMessage.text("Sign in failure. Please try again.").fadeIn();
                }
            });

            return false;
        });
    });
</script>
    </div>
    <script language="javascript" type="text/javascript">
        function ShowLoginDialog(comment, returnValue)
        {
            // this version is used with video presentations because otherwise it was a modal on top of a modal and 
            // was causing text inputs to not work so we are appending it to the div within the VideoPresentationModal.cshtml
            if ($('#saveToLibrarySignIn').length > 0) {
                $("#divPopupLogin").dialog(
                    {
                        width: "auto",
                        height: "auto",
                        top: "auto",
                        modal: true,
                        resizable: false,
                        draggable: false,
                        closeOnEscape: true,
                        zIndex: 999,
                        title: 'Sign In',
                        appendTo: "#saveToLibrarySignIn",
                        create: function (event, ui) {
                            $('.ui-dialog-titlebar-close').attr('aria-label', "Close Sign In Box");
                        }
                    }
                );
            }
            else {
                $("#divPopupLogin").dialog(
                    {
                        width: "auto",
                        height: "auto",
                        modal: true,
                        resizable: false,
                        draggable: false,
                        closeOnEscape: true,
                        zIndex: 999,
                        title: 'Sign In',
                        create: function (event, ui) {
                            $('.ui-dialog-titlebar-close').attr('aria-label', "Close Sign In Box");
                        }
                    }
                );
            }
            $("#EmailorUsername").focus(); //get this working on the library version.  Need to focus on email when modal opens.
            if (comment) {
                $("#SignInErrors").text(comment);
            }
            //Returning true processes the anchor tag's href link.
            //Returning false does not.
            if (returnValue === true) {
                return true;
            }
            else {
                return false;
            }
        }
    </script>


