<script type="text/javascript">

    function ShowEratumAlertDialog(doi, type)
    {


        $(".ui-dialog-content").dialog("close");
        $('#ErratumPaperId, #PaperToSave-ErratumAlertPopup').val(doi);
        $('#ErratumPaperType, #PaperToSaveType-ErratumAlertPopup').val(type);

        $("#divPopupEratumAlert").
            dialog(
                {
                    width: 360,
                    height: 600,
                    modal: true,
                    resizable: false,
                    draggable: false,
                    closeOnEscape:true,
                    zIndex: 999,
                    title: 'ERRATUM ALERT'
                }
            );

        return false;
    }

    function ShowEratumAlertConfirmationDialog()
    {
        $('.ui-dialog-content').dialog('close');
        $("#divPopupEratumAlertConfirmation").
            dialog(
                {
                    width: 360,
                    height: 260,
                    modal: true,
                    resizable: false,
                    draggable: false,
                    closeOnEscape:true,
                    zIndex: 999,
                    title: 'ERRATUM ALERT'
                }
            );

        return false;
    }

    function ShowEratumAlertFailureDialog()
    {
        $('.ui-dialog-content').dialog('close');
        $("#divPopupEratumAlertFailure").
            dialog(
                {
                    width: 360,
                    height: 260,
                    modal: true,
                    resizable: false,
                    draggable: false,
                    closeOnEscape:true,
                    zIndex: 999,
                    title: 'ERRATUM ALERT'
                }
            );

        return false;
    }

    $("#ErratumAlertForm").submit(function(e)
    {
        $.ajax({
            type: "POST",
            url: "/accountAjax/saveErratumAlert",
            data: $(this).serialize(),
            success: function(result)
            {
                if (result.Success)
                {
                    ShowEratumAlertConfirmationDialog();
                }
                else
                {
                    ShowEratumAlertFailureDialog();
                }
            },
            error : function(jqXHR, textStatus, errorThrown)
            {
                ShowEratumAlertFailureDialog();
            }
        });
        e.preventDefault();
    });

</script>


                    </div>
                </div>
            </div>
        </div>
